{{> header }}

<div id="contents">
  <div class="box">
    <div>
      <div class="body">
        <h1>Travel</h1>
        <ul id="sites">
          {{#each trips}}
            <li>
              <a href="#"><img src="/images/{{this.image}}" alt="Img"></a>
              <h2><a href="#">{{this.name}}</a></h2>
              <p>{{this.description}}</p>
            </li>
          {{/each}}
        </ul>
      </div>
    </div>
  </div>
</div>

{{> footer }}
